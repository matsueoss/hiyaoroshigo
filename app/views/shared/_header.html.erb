<header class="l_header">
  <div class="l_header_contents">
    <% unless profile %>
     <div class="profile-btn profile-back-btn">
       <a href="javascript:history.back();">
         <%= fa_icon "chevron-left" %>
       </a>
     </div>
    <% else %>
     <div class="profile-btn profile-show-btn">
       <%= link_to fa_icon("user"), "/drinkers/edit" %>
     </div>
    <% end %>
  </div>
</header>
